<section id="branding-message" class="row">
    <div class="span4 offset2">
        <h3>Creating surveys haven't been easier before.</h3>
        <h5>Analize, reuse, organize and archive.</h5><br>
        <button class="btn btn-large btn-success">Sign up for free</button>
    </div>
    <div class="span4">
        <%= image_tag("front/home-survey.jpeg", :width => "250", :alt => "Show Group" ) %>
    </div>
</section>

<section id="branding-survey-flow" class="row">
    <div class="span4">
        <%= image_tag("front/create-home.png") %>
        <p>Create a survey.</p>
    </div>
    <div class="span4">
        <%= image_tag("front/distribute-home.png") %>
        <p>Distribute it.</p>
    </div>
    <div class="span4">
        <%= image_tag("front/analize-home.png")  %>
        <p>Analize your data.</p>
    </div>
</section>
<form action="https://www.sandbox.paypal.com/cgi-bin/webscr" method="post">
    <input type="hidden" name="cmd" value="_s-xclick">
    <input type="hidden" name="hosted_button_id" value="JE6VBVQVEGQZQ">
    <input type="image" src="https://www.sandbox.paypal.com/en_US/i/btn/btn_subscribeCC_LG.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!">
    <img alt="" border="0" src="https://www.sandbox.paypal.com/es_XC/i/scr/pixel.gif" width="1" height="1">
</form>

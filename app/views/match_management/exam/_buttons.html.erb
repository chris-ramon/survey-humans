<%if @int_page_type==0%>
  <%= submit_tag(t('buttons.register'), :disable_with => t('buttons.processing'), :class=>"btn btn-success")%>          
<%elsif @int_page_type==1%>
  <%= link_to t('buttons.enable_edition'), edit_match_management_exam_path(@match), :class=>"button inline" unless !current_user.has_access(26) %>
<%else%>
  <%= submit_tag(t('buttons.edit'), :disable_with => t('buttons.processing'), :class=>"btn btn-success inline")%>      
<%end%>
<%= link_to t('buttons.cancel'), match_management_exam_index_path, :class=>"btn inline"%>
<br/>
<%= form_for(@panel_subscription) do |f| %>
  <% if @panel_subscription.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@panel_subscription.errors.count, "error") %> prohibited this panel_subscription from being saved:</h2>

      <ul>
      <% @panel_subscription.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :User %><br />
    <%= f.select :user_id, @users.collect {|t| [t.email, t.id]  } %>
  </div>
  <div class="field">
    <%= f.label :Plan %><br />
    <%= f.select :panel_plan_id, @plans.collect {|t| [t.name, t.id]} %>
  </div>
  <div class="field">
    <%= f.label :Deal %><br />
    <%= f.text_field :panel_deal_id %>
  </div>
  <div class="field">
    <%= f.label :expired_at %><br />
    <%= f.datetime_select :expired_at %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

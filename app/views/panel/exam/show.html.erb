<section id="panel">
    <br/><br/><br/>
    <legend><h3>Enter to <%=@match.name%>!</h3></legend>

    <%=form_for(@student, :url=>validate_user_panel_exam_index_path, :method=>"post") do |f| %>

    	<label>Email: </label> <%=f.text_field :email%><br/>
    	<label>Code: </label><%=f.password_field :code%><br/><br/>
    	<input type="hidden" name="match_id" value="<%=@match_id%>"/>
    	<%=f.submit "Enter", {:class=>"btn"}%> <br/><br/>
    	<p><a href="<%=forgot_password_panel_exam_index_path+'&id='+@match_id%>">Forgot your code?</a></p>
    	<p><a href="<%=new_code_panel_exam_index_path+'&id='+@match_id%>">Do not have a code?</a></p>
    <%end%>
    
</section>
<script src="http://code.jquery.com/ui/1.9.1/jquery-ui.js"></script>

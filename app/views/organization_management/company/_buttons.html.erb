<%if @int_page_type==0%>
  <%= submit_tag(t('buttons.register'), :disable_with => t('buttons.processing'), :class => "btn btn-success")%>          
<%elsif @int_page_type==1%>
  <%= link_to t('buttons.enable_edition'), edit_organization_management_company_path(@company), :class=>"button" unless !current_user.has_access(20) %>
<%else%>
  <%= submit_tag(t('buttons.edit'), :disable_with => t('buttons.processing'), :class => "btn btn-success inline")%>      
  <%= link_to t('buttons.undo_changes'), edit_organization_management_company_path(@company), :class=>"btn btn-success inline"  %>
<%end%>
<%= link_to t('buttons.cancel'), organization_management_company_index_path, :class=>"btn inline"%>
